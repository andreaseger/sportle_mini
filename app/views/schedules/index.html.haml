#schedules
  = render @schedules

= form_for Schedule.new, :remote => true do |f|
  .field
    = f.label :content
    = f.text_area :content
  .field
    = f.label :long_course, "Course"
    = f.radio_button :long_course, true
    50m
    = f.radio_button :long_course, false
    25m
  .field
    = f.label :date
    = f.text_field :date
  .actions
    = f.submit 'Save'
